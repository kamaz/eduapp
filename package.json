{
  "name": "eduapp",
  "private": true,
  "version": "0.1.0",
  "description": "Personalised learning for children with AI-generated activities aligned to the UK National Curriculum.",
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "dev:mobile": "turbo run dev --filter=mobile",
    "dev:worker": "turbo run dev --filter=worker",
    "test": "turbo run test",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
